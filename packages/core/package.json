{
  "name": "@hazy-up/core",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/main.cjs",
  "module": "./dist/main.js",
  "types": "./dist/types/es/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/es/index.d.ts",
        "default": "./dist/main.js"
      },
      "require": {
        "types": "./dist/types/cjs/index.d.cts",
        "default": "./dist/main.cjs"
      }
    },
    "./document": {
      "import": {
        "types": "./dist/types/es/document/index.d.ts",
        "default": "./dist/document.js"
      },
      "require": {
        "types": "./dist/types/cjs/document/index.d.cts",
        "default": "./dist/document.cjs"
      }
    },
    "./editor": {
      "import": {
        "types": "./dist/types/es/editor/index.d.ts",
        "default": "./dist/editor.js"
      },
      "require": {
        "types": "./dist/types/cjs/editor/index.d.cts",
        "default": "./dist/editor.cjs"
      }
    },
    "./theme": {
      "import": {
        "types": "./dist/types/es/theme/index.d.ts",
        "default": "./dist/theme.js"
      },
      "require": {
        "types": "./dist/types/cjs/theme/index.d.cts",
        "default": "./dist/theme.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "document": [
        "./dist/types/es/document/index.d.ts"
      ],
      "editor": [
        "./dist/types/es/editor/index.d.ts"
      ],
      "theme": [
        "./dist/types/es/theme/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && npm run cts",
    "cts": "cp -r dist/types/es dist/types/cjs && find ./dist/types/cjs -maxdepth 2 -name index.d.ts | sed 'p;s/d.ts/d.cts/g' |  xargs -n2 mv",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "nanoid": "^4.0.2"
  },
  "devDependencies": {
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/react": "^2.6.1",
    "@emotion/css": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@types/node": "^20.2.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "framer-motion": "^10.12.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vite-plugin-dts": "^2.3.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "peerDependencies": {
    "@chakra-ui/icons": "^2.0.19",
    "@chakra-ui/react": "^2.6.1",
    "@emotion/css": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^10.12.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "typescript": "^5.0.2"
  },
  "license": "MIT"
}
